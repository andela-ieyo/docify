{
  "name": "docify",
  "version": "1.0.0",
  "description": "A document management application",
  "main": "server.js",
  "jest": {
    "globals": {
      "window": true
    }
  },
  "scripts": {
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha ./tests/backend/*.js --require babel-register",
    "start:dev": "webpack -w & nodemon server.js --exec babel-node",
    "db:migrate": "NODE_ENV=development sequelize db:migrate:undo:all && sequelize db:migrate",
    "build": "rm -rf public/ && webpack --config webpack.config.prod.js",
    "postinstall": "npm run build",
    "test:frontend": "jest --coverage",
    "start": "node distServer.js"
  },
